{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "@delandlabs/crypto-lib#build:esm": {
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/crypto-lib#build:common": {
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-base#build": {
      "outputs": [
        "dist/**"
      ],
      "dependsOn": [
        "@delandlabs/crypto-lib#build:esm"
      ]
    },
    "@delandlabs/coin-ethereum#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-ton#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-dfinity#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-kaspa#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build",
        "@delandlabs/crypto-lib#build:esm"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-kaspa#test": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build",
        "@delandlabs/crypto-lib#build:common"
      ],
      "outputs": [
        "dist/**"
      ],
      "persistent": true
    },
    "@delandlabs/coin-solana#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/coin-tron#build": {
      "env": [
        "VITE_*"
      ],
      "dependsOn": [
        "@delandlabs/coin-base#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "@delandlabs/hibit-id-wallet-sdk#build": {
      "env": [
        "VITE_*"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {},
    "test:watch": {}
  }
}
